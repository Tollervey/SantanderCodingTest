{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "CacheSettings": {
    "TimeoutInMinutes": 2
  },
  "HackerNewsSettings": {
    "BaseUrl": "https://hacker-news.firebaseio.com/v0",
    "BestStoriesEndpoint": "/beststories.json",
    "StoryDetailsEndpoint": "/item/{0}.json",
    "MaxConcurrentRequests":  100
  },
  "AllowedHosts": "*"
}
